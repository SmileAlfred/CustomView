        android:theme="@style/AppTheme">
        <activity android:name=".youku.menu.MenuActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".youku.menu.YoukuMenu" />
        <activity android:name=".advance.AdvanceItem" />
