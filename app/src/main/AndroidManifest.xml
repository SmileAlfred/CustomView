<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.customview">

    <application
        android:allowBackup="true"
        android:icon="@drawable/qq_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/qq_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".YoukuMenu" />
        <activity android:name=".AdvanceItem" />
        <activity android:name=".PopupWindowsItem" />
        <activity android:name=".AutoAttributeItem" />
        <activity android:name=".MyToggleButtonItem" />
        <activity android:name=".MyViewPagerItem" />
        <activity android:name=".QuickIndexItem"></activity>
    </application>

</manifest>